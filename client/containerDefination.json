{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "complex",
    "containerDefinitions": [
        {
            "name": "client",
            "hostname": "client",
            "image": "sahilsaxena91096/client",
            "essential": false,
            "memory": 128
        },
        {
            "name": "server",
            "hostname": "api",
            "image": "sahilsaxena91096/server",
            "essential": false,
            "memory": 128
        },
        {
            "name": "worker",
            "hostname": "worker",
            "image": "sahilsaxena91096/worker",
            "essential": false,
            "memory": 128
        },
        {
            "name": "nginx",
            "hostname": "nginx",
            "image": "sahilsaxena91096/nginx",
            "essential": true,
            "memory": 128,
            "portMappings":[
                {
                    "containerPort": 80,
                    "hostPort": 80
                }
            ],
            "links":["client","api"]
        }
    ],
    
    "networkMode": "awsvpc",
    "memory": "1 GB",
    "cpu": ".5 vCPU",
    "executionRoleArn": ""
}